<MudCard Class="scrabble-tile" style="height: 100%; width: 100%;" >
    <MudCardContent Style="background-color:peru">
        <div class="tile-letter">@Letter</div>
        <div class="tile-score">@Score</div>
    </MudCardContent>
</MudCard>

@code {
    [Parameter] public char Letter { get; set; }
    [Parameter] public int Score { get; set; }
}